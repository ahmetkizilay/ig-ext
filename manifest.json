{
    "manifest_version": 2,

    "name": "Instagram Viewer",

    "description": "An extension to view Instagram images",

    "version": "0.1",

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_popup": "pages/feed.html",
        "default_icon": "ig-badge-16.png"
    },

    "permissions": [
        "identity",
        "storage",
        "https://api.instagram.com/",
        "https://instagram.com/",
        "http://localhost:2424/"
    ]

}